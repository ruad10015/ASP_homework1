@model List<homework1.Entities.FastFood>


<!DOCTYPE html>
<html>
<head>

</head>
<body style="background-color:gray">
    <hr />
    <h1 style="color:darkblue">Fast Foods</h1>
    <hr />

    @foreach (var food in Model)
    {
        <div>
            <h1>ID : @food.Id</h1>
            <h1>Food Name : @food.Name</h1>
            <h1>Food Description : @food.Description</h1>
            <h1>Food Price: @food.Price</h1>
            <hr />
        </div>

    }
</body>
</html>